<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.pusku.smartmonit.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>clientBase</servlet-name>
        <servlet-class>com.pusku.smartmonit.clientBase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>new_client</servlet-name>
        <servlet-class>com.pusku.smartmonit.new_client</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_client</servlet-name>
        <servlet-class>com.pusku.smartmonit.add_client</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>error_add_client</servlet-name>
        <servlet-class>com.pusku.smartmonit.error_add_client</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>clientBase</servlet-name>
        <url-pattern>/clientBase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>new_client</servlet-name>
        <url-pattern>/new_client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_client</servlet-name>
        <url-pattern>/add_client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>error_add_client</servlet-name>
        <url-pattern>/error_add_client</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
